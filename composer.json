{
    "name": "kolovious/meli-socialite",
    "description": "Mercadolibre Laravel Socialite Integration",
    "keywords": [
        "Laravel",
        "Socialite",
        "Mercadolibre"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Garcia",
            "email": "cololeo@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "mercadolibre/php-sdk",
                "version": "master",
                "source": {
                    "url": "https://github.com/mercadolibre/php-sdk.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "laravel/socialite": "4.*",
        "illuminate/support": "7.*",
        "mercadolibre/php-sdk": "master"
    },
    "autoload": {
        "psr-4": {
            "Kolovious\\MeliSocialite\\": "src/"
        },
        "files": [
            "vendor/mercadolibre/php-sdk/Meli/meli.php"
        ]
    }
}
